local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Netflix Hub",
   LoadingTitle = "Welcome",
   LoadingSubtitle = "Coded By revvxs",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Revvxs'sHUB"
   },
   KeySystem = false, -- Set this to true to use our key system
})

local MainTab = Window:CreateTab("Page (1)", nil) -- Title, Image
local MainSection = MainTab:CreateSection("1")
local SecondTab = Window:CreateTab("Page (2)", nil) -- Title, Image
local SecondSection = SecondTab:CreateSection("2")
local ThirdTab = Window:CreateTab("Page (3)", nil) -- Title, Image
local ThirdSection = ThirdTab:CreateSection("3")

